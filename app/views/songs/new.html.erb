<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_name, Genre.all, :id, :name %>

  <%= f.label :note_contents %>
  <%= f.text_field :note_contents %>

  <%= f.submit %>
<% end %>


